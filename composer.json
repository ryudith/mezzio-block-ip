{
    "name": "ryudith/mezzio-block-ip",
    "description": "Middleware library to block IP request for Mezzio framework.",
    "authors": [
        {
            "name": "Ryudith"
        }
    ],
    "license": "Apache-2.0",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Ryudith\\MezzioBlockIp\\": "src/"
        }
    },
    "autoload-dev" : {
        "psr-4": {
            "Ryudith\\MezzioBlockIp\\AppTest\\": "tests/"
        }
    },
    
    "require": {
        "php": "^8.0.0",
        "psr/http-server-middleware": "^1.0",
        "psr/container": "^1.0",
        "laminas/laminas-diactoros": "^2.11",
        "symfony/console": "^6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.11"
    }
}
